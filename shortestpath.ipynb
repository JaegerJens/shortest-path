   "source": [
    "import numpy as np\n",
    "\n",
    "coords = np.array([[-1, -1], [1, -1], [1, 1], [-1, 1]])\n",
    "\n",
    "center = np.mean(coords, axis=0)\n",
    "\n",
    "distances = np.linalg.norm(coords - center, axis=1)\n",
    "\n",
    "normalizedCoords = coords / distances[:, np.newaxis]\n",
    "\n",
    "print(normalizedCoords)\n"
   ]
  }
